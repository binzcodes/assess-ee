{
  "name": "ee-test",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "type": "module",
  "engines": {
    "node": ">=0.12"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist --ignore **/*.spec.js",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "jest --no-cache --verbose ./src",
    "test:coverage": "jest --coverage ./src",
    "lint": "npx prettier --write ."
  },
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {},
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "axios": "^1.2.5",
    "express": "^4.13.3",
    "express-async-handler": "^1.2.0",
    "msw": "^2.7.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.3.1",
    "nodemon": "^3.1.7",
    "prettier": "3.4.2",
    "supertest": "^7.1.0"
  }
}
